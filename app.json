{
  "expo": {
    "name": "interview-react-native-kompit",
    "slug": "interview-react-native-kompit",
    "version": "1.0.0",
    "orientation": "portrait",

    "scheme": "myapp",
    "userInterfaceStyle": "automatic",

    "extra": {
      "apiUrl": "https://staging.example.com"
    },
    "ios": {
      "supportsTablet": true
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
