{
  "expo": {
    "name": "interview-react-native-kompit",
    "slug": "interview-react-native-kompit",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "extra": {
      "apiUrl": "https://staging.example.com",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b2c73ee6-15b0-4209-8ad1-5e852f119234"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/Poppins-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.babangciko.interviewreactnativekompit"
    }
  }
}
